<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.insightfullogic.honest_profiler.ports.javafx.controller.TreeDiffViewController">
   <children>
      <HBox alignment="CENTER_LEFT" maxHeight="25.0" maxWidth="1.7976931348623157E308" minHeight="25.0" prefHeight="25.0" spacing="6.0">
         <children>
            <GridPane>
               <columnConstraints>
                  <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="25.0" minWidth="25.0" prefWidth="25.0" />
                  <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="25.0" minWidth="25.0" prefWidth="25.0" />
                  <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="25.0" minWidth="25.0" prefWidth="25.0" />
                  <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="25.0" minWidth="25.0" prefWidth="25.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="25.0" prefWidth="200.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints maxHeight="25.0" minHeight="25.0" prefHeight="25.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Button fx:id="filterButton" alignment="CENTER" maxHeight="20.0" maxWidth="20.0" minHeight="20.0" minWidth="20.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="20.0" GridPane.hgrow="NEVER" GridPane.vgrow="NEVER" />
                  <Button fx:id="expandAllButton" maxHeight="20.0" maxWidth="20.0" minHeight="20.0" minWidth="20.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="20.0" GridPane.columnIndex="1" GridPane.hgrow="NEVER" GridPane.vgrow="NEVER" />
                  <Button fx:id="collapseAllButton" maxHeight="20.0" maxWidth="20.0" minHeight="20.0" minWidth="20.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="20.0" GridPane.columnIndex="2" GridPane.hgrow="NEVER" GridPane.vgrow="NEVER" />
                  <HBox maxHeight="20.0" minHeight="20.0" prefHeight="20.0" GridPane.columnIndex="4">
                     <children>
                        <TextField fx:id="quickFilterText" maxHeight="20.0" maxWidth="1.7976931348623157E308" minHeight="20.0" prefHeight="20.0" promptText="Quick FQMN Filter" HBox.hgrow="ALWAYS" />
                        <Button fx:id="quickFilterButton" alignment="CENTER" maxHeight="20.0" maxWidth="20.0" minHeight="20.0" minWidth="20.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="20.0" HBox.hgrow="NEVER" />
                     </children>
                  </HBox>
               </children>
            </GridPane>
         </children>
         <padding>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </padding>
      </HBox>
      <TreeTableView fx:id="diffTree" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" showRoot="false" VBox.vgrow="ALWAYS">
         <columns>
            <TreeTableColumn fx:id="methodColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="-1.0" sortable="false" text="Method" />
            <TreeTableColumn fx:id="baseSelfPct" editable="false" maxWidth="1000.0" minWidth="60.0" prefWidth="-1.0" sortable="false" text="Base Self %" />
            <TreeTableColumn fx:id="newSelfPct" editable="false" maxWidth="1000.0" minWidth="60.0" prefWidth="-1.0" sortable="false" text="New Self %" />
            <TreeTableColumn fx:id="selfPctDiff" maxWidth="1000.0" minWidth="60.0" prefWidth="-1.0" text="Self % Diff" />
            <TreeTableColumn fx:id="baseTotalPct" maxWidth="1000.0" minWidth="60.0" prefWidth="-1.0" text="Base Total %" />
            <TreeTableColumn fx:id="newTotalPct" maxWidth="1000.0" minWidth="60.0" prefWidth="-1.0" text="New Total %" />
            <TreeTableColumn fx:id="totalPctDiff" maxWidth="1000.0" minWidth="60.0" prefWidth="-1.0" text="Total % Diff" />
            <TreeTableColumn fx:id="baseSelfCount" maxWidth="1000.0" minWidth="60.0" prefWidth="-1.0" text="Base Self #" />
            <TreeTableColumn fx:id="newSelfCount" maxWidth="1000.0" minWidth="60.0" prefWidth="-1.0" text="New Self #" />
            <TreeTableColumn fx:id="selfCountDiff" maxWidth="1000.0" minWidth="60.0" prefWidth="-1.0" text="Self # Diff" />
            <TreeTableColumn fx:id="baseTotalCount" maxWidth="1000.0" minWidth="60.0" prefWidth="-1.0" text="Base Total #" />
            <TreeTableColumn fx:id="newTotalCount" maxWidth="1000.0" minWidth="60.0" prefWidth="-1.0" text="New Total #" />
            <TreeTableColumn fx:id="totalCountDiff" maxWidth="1000.0" minWidth="60.0" prefWidth="-1.0" text="Total # Diff" />
            <TreeTableColumn fx:id="baseParentCount" maxWidth="1000.0" minWidth="60.0" prefWidth="-1.0" text="Base Parent #" />
            <TreeTableColumn fx:id="newParentCount" maxWidth="1000.0" minWidth="60.0" prefWidth="-1.0" text="New Parent #" />
            <TreeTableColumn fx:id="parentCountDiff" maxWidth="1000.0" minWidth="60.0" prefWidth="-1.0" text="Parent # Diff" />
         </columns>
         <columnResizePolicy>
            <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy></TreeTableView>
   </children>
</VBox>
